{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
			  "quotes": ["error", "double"],
				"semi": "error",
				"no-unused-vars":"error",
				"no-console": ["error", {"allow": ["warn", "error"]}]
    }
}
